--TEST--
Integration of fixers: simplified_null_return,no_useless_return,no_whitespace_in_blank_lines,blank_line_before_return,no_extra_consecutive_blank_lines.
--CONFIG--
{
    "simplified_null_return": true,
    "no_useless_return": true,
    "no_whitespace_in_blank_lines": true,
    "blank_line_before_return": true,
    "no_extra_consecutive_blank_lines": true
}
--EXPECT--
<?php
class Test {

    private static function a($a) {
        if ($a) {
            return 1;
        }
        /**/
        ++$a; //
    }

    private static function b($a) {
        if ($a) {
            return;
        }

    }
}

--INPUT--
